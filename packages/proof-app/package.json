{
  "name": "@wifiproof/proof-app",
  "version": "0.1.0",
  "description": "Zero-Knowledge geolocation proof generation for WiFiProof V2",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "circuit/target"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "clean": "rm -rf dist",
    "generate:claim": "node scripts/generate-claim-input.mjs",
    "circuit:compile": "cd circuit && nargo compile",
    "circuit:test": "cd circuit && nargo test",
    "circuit:prove": "cd circuit && nargo execute"
  },
  "dependencies": {
    "@noir-lang/noir_js": "1.0.0-beta.18",
    "@aztec/bb.js": "3.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "buffer": "^6.0.0"
  },
  "author": "Xiaomao <https://github.com/wamimi>",
  "license": "MIT"
}
